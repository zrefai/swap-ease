    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "swap-ease-utils": "file:../../libraries/utils",
    "viem": "^1.19.15"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
